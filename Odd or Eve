/* To say if number is even or odd between n and m */
#include <stdio.h>
int main()
{
    int i,n,m;

    printf("Enter n: ");
    scanf("%d",&n);

    printf("Enter m: ");
    scanf("%d",&m);

    for(i = n; i <= m; i++)
    {
        if(i % 2 == 0)
            printf("\n%d is even",i);
        else
            printf("\n%d is odd",i);
    }
    return 0;
}
