/* to print the following pattern 
* * * * * * * 
* * * * * * *
* * * * * * *
* * * * * * *
* * * * * * *
* * * * * * *  */
#include <stdio.h>
int main()
{
    int i,j;
    for(i = 1; i <= 6; i++){
        printf("* ");
        for(j = 1; j <= 6; j++)
        {
            printf("* ",j);
        }
        printf("\n");
    }
}
