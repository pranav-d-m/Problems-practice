#include <stdio.h>
void swap(int a, int b)
{
    int temp;
    temp = a;
    a = b;
    b = temp;
    printf("Now a is %d and b is %d",a,b);
}
int main() 
{
    int num_1, num_2;

    printf("Enter first number: ");
    scanf("%d",&num_1);
    printf("Enter second number: ");
    scanf("%d",&num_2);

    swap(num_1,num_2);

    return 0;
