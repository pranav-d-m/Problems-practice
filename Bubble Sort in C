#include <stdio.h>
int main()
{
    int n,i,j;

    printf("Enter the number of array elements: ");
    scanf("%d",&n);

    int arr[n];

    printf("Enter the elements: ");

    for(i = 0; i < n; i++)
    {
        scanf("%d",&arr[i]);
    }

    for(i = 0; i < n; i++)
    {
        for(j = 1; j < n; j++)
        {
            if(arr[j-1] > arr[j])
            {
                int temp = arr[j-1];
                arr[j-1] = arr[j];
                arr[j] = temp;
            }
        }
    }

    printf("The array of ssorted elements are: ");
    for(i = 0; i < n; i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
