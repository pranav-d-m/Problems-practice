/*To get this pattern
   1
  12
 123
1234
*/
#include <stdio.h>
int main() 
{
    int i, j, n = 4; 

    for (i = 1; i <= n; i++) 
    {
        for(j = 1; j <= n; j++)
        {
            if(j <= n - i)
                printf(" ");
            else
                printf("%d", j - (n - i));
        }
        printf("\n"); 
    }
    return 0;
}
