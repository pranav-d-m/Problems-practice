#include <stdio.h>
int main() 
{ 
    int n,sum = 0;

    printf("Enter the number till which you want to find out the sum of multiples of 2: ");
    scanf("%d",&n);

    for(int i = 0; i <= n; i++)
    {
        if(i % 2 == 0)
        {
            sum += i;
        }
    }
    printf("The sum of even numbers till n is: %d",sum);
    return 0;
}
