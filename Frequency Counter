#include <stdio.h>

int main() 
{ 
    int n,i,count_2 = 0,count_3 = 0,count_4 = 0;


    int arr[6] = {2,3,2,4,3,3};

    for(int j = 0; j < 6; j++)
    {
        for(int i = 0; i < 6; i++)
        {
            if(2 == arr[i])
            {
                count_2 += 1;
            }
            else if(3 == arr[i])
            {
                count_3 += 1;
            }
            else if(4 == arr[i])
            {
                count_4 += 1;
            }
        }
    }
    printf("The number od 2's are: %d\n",count_2);
    printf("The number od 3's are: %d\n",count_3);
    printf("The number od 4's are: %d\n",count_4);
    return 0;
}
