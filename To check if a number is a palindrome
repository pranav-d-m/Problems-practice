#include <stdio.h>
#include<string.h>
#include <math.h>
#include<stdlib.h>
int main()
{
    int i,palindrome;
    char num[100];
    printf("Enter num value: ");
    scanf("%s", num);

    int leng = strlen(num);

    for(i = 0; i < leng; i++)
        for(int j = leng-1-i ; j >= 0; j--)
        {
            if(num[i] == num[j])
            {
                palindrome = 1;
            }
            else
            {
                palindrome = 0;
            }
        }
    if(palindrome == 1)
    {
        printf("Palindrome");
    }
    else
    {
        printf("Not a Palindrome");
    }
    
}
