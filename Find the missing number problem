import java.util.Scanner;
class practice{
    public static void main(String args[])
    {
        int sum = 0, n;

        Scanner sc = new Scanner(System.in);

        System.out.print("Enter the n value: ");
        n = sc.nextInt();

        int[] arr = new int[n-1];
        for(int i = 0; i < n-1; i++){
            arr[i] = sc.nextInt();
            sum += arr[i];
        }
        
        int total = n*(n+1)/2;
        System.out.println("The missing number is "+(total-sum));
    }
}
