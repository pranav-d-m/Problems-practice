#include <stdio.h>
int main() 
{
    int arr[10],sum = 0,arr_2[10];

    printf("Enter the array elements: ");
    for(int i = 0, j = 10; i < 10, j >= 0; i++, j--)
    {
        scanf("%d",&arr[i]);
        arr_2[j] = arr[i];
    }
    printf("Array 2 = ");
    for(int k = 0; k < 10; k++)
    {
        printf("%d ",arr_2[k]);
    }
}
