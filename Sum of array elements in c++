#include <iostream>
#include <vector>
using namespace std;

int main()
{
    vector<int> arr;
    int sum = 0, input;

    cout << "Enter 10 array elements: " << endl;
    for(int i = 0; i < 10; i++) {
        cin >> input;
        arr.push_back(input); 
    }

    for(int i = 0; i < 10; i++) {
        sum += arr[i];
    }

    cout << "Sum = " << sum << endl;
    return 0;
}
