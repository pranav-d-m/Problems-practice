#include <stdio.h>

int main() 
{
    int arr[100]; 
    int n, i, newElement;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements: ", n);
    for(i = 0; i < n; i++) 
    {
        scanf("%d", &arr[i]);
    }

    printf("Enter the element to insert at last: ");
    scanf("%d", &newElement);

    arr[n] = newElement; 
    n++; 

    printf("Array after insertion: ");
    for(i = 0; i < n; i++)
    {
        printf("%d ", arr[i]);
    }

    return 0;
}
