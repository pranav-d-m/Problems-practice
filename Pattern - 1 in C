//To create the following pattern 
/*Pass:-1 1 2 3 4 5 6 
Pass:-2 1 2 3 4 5 6 
Pass:-3 1 2 3 4 5 6 
Pass:-4 1 2 3 4 5 6 
Pass:-5 1 2 3 4 5 6 
Pass:-6 1 2 3 4 5 6 */
#include <stdio.h>
#include<string.h>
#include <math.h>
#include<stdlib.h>
int main()
{
    int i,j;
    for(i = 1; i <= 6; i++){
        printf("Pass:-%d ",i);
        for(j = 1; j <= 6; j++)
        {
            printf("%d ",j);
        }
        printf("\n");
    }
}
