/* To print this pattern 
* * * * * * 
* * * * * * 
* * * * * * 
* * * * * * 
* * * * * * 
* * * * * *
*/
#include <stdio.h>
int main() 
{
    int i, j, n = 5; 
    
    for(i = 0; i <= n; i++)
    {
        printf("\n");
        for(j = 0; j <= n; j++)
        {
            printf("* ");
        }
    }
}
